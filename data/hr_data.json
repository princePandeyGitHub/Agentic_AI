  "policies": [
    {
      "policy_id": "POL001",
      "title": "Annual Leave Policy",
      "description": "Employees are entitled to annual leave based on tenure. New employees receive 10 days, increasing by 2 days per year of service up to a maximum of 30 days. Leave must be requested at least 2 weeks in advance for periods longer than 3 days.",
      "category": "leave"
    },
    {
      "policy_id": "POL002",
      "title": "Sick Leave Policy",
      "description": "All employees receive 12 sick days per year. Unused sick days do not carry over. Medical certificate required for absences longer than 3 consecutive days.",
      "category": "leave"
    },
    {
      "policy_id": "POL003",
      "title": "Remote Work Policy",
      "description": "Eligible employees may work remotely up to 3 days per week with manager approval. Remote work stipend of $50/month available for home office expenses.",
      "category": "work_arrangement"
    },
    {
      "policy_id": "POL004",
      "title": "Expense Reimbursement Policy",
      "description": "Business expenses must be submitted within 30 days with receipts. Meals up to $50/day during travel. Mileage reimbursed at $0.655 per mile.",
      "category": "finance"
    },
    {
      "policy_id": "POL005",
      "title": "401k Retirement Plan",
      "description": "Company matches 100% of employee contributions up to 4% of salary. Vesting schedule: 25% per year, fully vested after 4 years.",
      "category": "benefits"
    },
    {
      "policy_id": "POL006",
      "title": "Health Insurance",
      "description": "Company covers 80% of premium for employee and 50% for dependents. Plans include medical, dental, and vision coverage. Open enrollment in November.",
      "category": "benefits"
    },
    {
      "policy_id": "POL007",
      "title": "Bonus Structure",
      "description": "Annual bonuses paid in Q1 based on individual and company performance. Bonus percentage varies by role level: Entry 5-8%, Mid 8-15%, Senior 15-25%.",
      "category": "compensation"
    },
    {
      "policy_id": "POL008",
      "title": "Maternity/Paternity Leave",
      "description": "Primary caregivers receive 12 weeks paid leave. Secondary caregivers receive 4 weeks paid leave. Additional unpaid leave available up to 12 weeks.",
      "category": "leave"
    }
